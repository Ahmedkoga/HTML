_render()},i.prototype._createScene=function(){this.raycaster=new t.Raycaster,this.renderer=i.SYSTEM.isWebGLSupported&&this.config.webgl?new t.WebGLRenderer:new t.CanvasRenderer,this.renderer.setSize(this.prop.size.width,this.prop.size.height),this.renderer.setPixelRatio(i.SYSTEM.pixelRatio);var e=i.SPHERE_RADIUS;this.prop.isCubemap&&(e*=Math.sqrt(3)),this.config.fisheye&&(e+=i.SPHERE_RADIUS),this.camera=new t.PerspectiveCamera(this.config.default_fov,this.prop.size.width/this.prop.size.height,1,e),this.camera.position.set(0,0,0),this.scene=new t.Scene,this.scene.add(this.camera),this.prop.isCubemap?this.mesh=this._createCubemap():this.mesh=this._createSphere(),this.scene.add(this.mesh),this.canvas_container=document.createElement("div"),this.canvas_container.className="psv-canvas-container",this.renderer.domElement.className="psv-canvas",this.container.appendChild(this.canvas_container),this.canvas_container.appendChild(this.renderer.domElement)},i.prototype._createSphere=function(e){e=e||1;var n=new t.SphereGeometry(i.SPHERE_RADIUS*e,i.SPHERE_VERTICES,i.SPHERE_VERTICES,-E.HalfPI),r=new t.MeshBasicMaterial({side:t.DoubleSide,overdraw:i.SYSTEM.isWebGLSupported&&this.config.webgl?0:1}),o=new t.Mesh(n,r);return o.scale.x=-1,o},i.prototype._setSphereCorrection=function(t,e){this.cleanSphereCorrection(e),t.rotation.set(e.tilt,e.pan,e.roll)},i.prototype._createCubemap=function(e){e=e||1;for(var n=new t.BoxGeometry(2*i.SPHERE_RADIUS*e,2*i.SPHERE_RADIUS*e,2*i.SPHERE_RADIUS*e,i.CUBE_VERTICES,i.CUBE_VERTICES,i.CUBE_VERTICES),r=[],o=0;o<6;o++)r.push(new t.MeshBasicMaterial({side:t.BackSide,overdraw:i.SYSTEM.isWebGLSupported&&this.config.webgl?0:1}));var a=new t.Mesh(n,r);return a.position.x-=i.SPHERE_RADIUS*e,a.position.y-=i.SPHERE_RADIUS*e,a.position.z-=i.SPHERE_RADIUS*e,a.applyMatrix((new t.Matrix4).makeScale(1,1,-1)),a},i.prototype._transition=function(e,n){var i,r=this.isExtendedPosition(n),o=void 0!==n.zoom;if(this.prop.isCubemap?(r&&(console.warn("PhotoSphereViewer: can